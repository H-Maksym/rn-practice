{
  "expo": {
    "name": "MePhoto",
    "slug": "MePhoto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/icon/MePhoto.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/image/backgroundImage.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "src/assets/favicon.png"
    }
  }
}
